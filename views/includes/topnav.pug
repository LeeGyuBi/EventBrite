.header.clearfix
    nav.navbar.navbar-expand-lg
      #title.navbar-brand
              a#text_title(href='/') Eventbrite
      |     
      button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarNavDropdown', aria-controls='navbarNavDropdown', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
          img.category(src='images/category.png', alt='category')
      |     
      #navbarNavDropdown.collapse.navbar-collapse
        ul.navbar-nav.ml-auto
          li.nav-item.active
            a.nav-link(href='#')
              | BROWSE EVENTS
              span.sr-only (current)
          |         
          li.nav-item
            a.nav-link(href="/users") USERS
          if (!currentUser) 
            li.nav-item
              a.nav-link(href='/signin') SIGN IN
            li.nav-item
              a.nav-link(href='/users/new') SIGN UP
          else
            li.nav-item
              a.nav-link(href=`/users/${currentUser._id}`) MY PROFILE
            li.nav-item
              a.nav-link(href='/signout') SIGN OUT
          li.nav-item
            a.nav-link(href='/users/new') SIGN UP         

          li.nav-item
            a#CREATE.nav-link(href='/questions') CREATE EVENT